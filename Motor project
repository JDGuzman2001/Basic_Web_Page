                        TFT.locate(50,15);
                        TFT.set_font((unsigned char*) Arial28x28);
                        printf("10");

                        TFT.locate(50,155);
                        TFT.set_font((unsigned char*) Arial28x28);
                        printf("0");

                        TFT.locate(50,290);
                        TFT.set_font((unsigned char*) Arial28x28);
                        printf("-10");

                        TFT.locate(80,130);
                        TFT.set_font((unsigned char*) Arial28x28);
                        printf("Velocidad");



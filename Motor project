                        TFT.rect(10,200,230,250,Red);
                        TFT.rect(10,260,230,310,Red);

                        TFT.set_font((unsigned char*) Arial12x12);
                        TFT.locate(16,10);
                        printf(" Universidad Distrital ");
                        TFT.locate(70,20);
                        printf(" Facultad Tecnologica ");
                        TFT.locate(20, 70);
                        printf("Juan Diego Guzman Jaimes");
                        TFT.locate(80, 90);
                        printf("20201573047");
                        TFT.locate(80, 160);
                        printf("Motor DC");
                        TFT.locate(70, 220);
                        printf("Servomotor");
                        TFT.locate(60, 280);
                        printf("Paso a paso");
                        estado=0;
                        digit=0;
                    }
                }
                //cierre paso a paso
            }

        }

    }
}

int main()
{

    BusOut  motor(D10, D6, D3, D2);
    PwmOut dc1(D14);
    PwmOut dc2(D15);
    PwmOut servo(A3);
    dc1.period(0.02);
    dc2.period(0.02);
    servo.period(0.020);
    TFT.claim(stdout);      // arranca el stdout de la TFT
    //TFT.claim(stderr);      // send stderr to the TFT display
    TFT.set_orientation(2);
    TFT.background(White);    // set background to black
    TFT.foreground(Black);    // set chars to white
    TFT.cls();                // clear the screen

    myTouch.InitTouch();
    myTouch.SetPrecision(PREC_LOW);


    if(estado==0) {
        TFT.rect(10,140,230,190,Red);
        TFT.rect(10,200,230,250,Red);
        TFT.rect(10,260,230,310,Red);


        TFT.set_font((unsigned char*) Arial12x12);

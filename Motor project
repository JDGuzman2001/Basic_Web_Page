        printf(" Facultad Tecnologica ");
        TFT.locate(20, 70);
        printf("Juan Diego Guzman Jaimes");
        TFT.locate(80, 90);
        printf("20201573047");
        TFT.locate(80, 160);
        printf("Motor DC");
        TFT.locate(70, 220);
        printf("Servomotor");
        TFT.locate(60, 280);
        printf("Paso a paso");
    }


    myTouch.InitTouch();
    myTouch.SetPrecision(PREC_LOW);

    while (true) {

        while(aux==1) {
            pas=pas-1;
            if(pas==0) {
                aux=0;
            } else {
                for(int i=0; i<4; i++) {
                    motor=A[i];
                    wait(0.01);
                }
            }
        }

        if (myTouch.DataAvailable()) {
            if(myTouch.Read()) {
                y = myTouch.GetX(); // toma x del touch
                x = myTouch.GetY(); // toma y del touch
                TFT.locate(170,11);
                TFT.set_font((unsigned char*) Arial12x12);
                sprintf (buffer, "%.0f , %.0f  ", x, y);
                TFT.printf(buffer);

                //aperura fase 0
                if(estado==0) {

                    //apertura fase 1
